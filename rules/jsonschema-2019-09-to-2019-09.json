      }
    },
    {
      "vocabulary": "https://json-schema.org/draft/2019-09/vocab/applicator",
      "condition": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "$id": "https://github.com/jviotti/alterschema/rules/jsonschema-2019-09-to-2019-09/empty-dependent-properties",
        "type": "object",
        "required": [ "dependentProperties" ],
        "properties": {
          "dependentProperties": {
            "const": {}
          }
        }
      },
      "transform": {
        "$eval": "omit(schema, 'dependentProperties')"
      }
    }
  ]
}
